{
    "name": "stechstudio/laravel-visual-testing",
    "description": "Visual UI testing for Laravel Dusk using percy.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "require": {
        "laravel/dusk": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "STS\\VisualTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\VisualTesting\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\VisualTesting\\VisualTestingServiceProvider"
            ]
        }
    }
}
